<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".SetRegulatorParameters">

    <!-- TODO: Update blank fragment layout -->
    <Button
        android:id="@+id/btnSendKp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_marginStart="15dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="100dp"
        android:text="send KP"
        tools:layout_editor_absoluteX="273dp"
        tools:layout_editor_absoluteY="87dp" />

    <Button
        android:id="@+id/btnSendKi"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="send Ki"
        android:layout_alignParentStart="true"
        android:layout_marginStart="125dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="100dp"
        tools:layout_editor_absoluteX="282dp"
        tools:layout_editor_absoluteY="187dp" />
    <Button
        android:id="@+id/btnSendKd"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="send Kd"
        android:layout_alignParentStart="true"
        android:layout_marginStart="250dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="100dp"
        tools:layout_editor_absoluteX="273dp"
        tools:layout_editor_absoluteY="304dp" />

    <EditText
        android:id="@+id/insertKp"
        android:layout_width="75dp"
        android:layout_height="50dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="22dp"
        android:layout_marginTop="48dp"
        android:inputType="numberDecimal"
        tools:layout_editor_absoluteX="160dp"
        tools:layout_editor_absoluteY="87dp" />

    <EditText
        android:id="@+id/insertKi"
        android:layout_width="75dp"
        android:layout_height="50dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="125dp"
        android:layout_marginTop="50dp"
        android:inputType="numberDecimal"
        tools:layout_editor_absoluteX="160dp"
        tools:layout_editor_absoluteY="187dp" />

    <EditText
        android:id="@+id/insertKd"
        android:layout_width="75dp"
        android:layout_height="50dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="250dp"
        android:layout_marginTop="50dp"
        android:inputType="numberDecimal"
        tools:layout_editor_absoluteX="161dp"
        tools:layout_editor_absoluteY="305dp" />


</RelativeLayout>